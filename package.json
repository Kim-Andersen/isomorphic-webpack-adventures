{
  "name": "isomorphic-react-relay-boilerplate",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "config": {
    "appMongoConnection": "mongodb://localhost:27017/hola",
    "appWebpackBaseUrl": "http://localhost:8080",
    "appWebpackBuildPath": "public/assets",
    "appServerHost": "127.0.0.1",
    "appServerPort": 3000
  },
  "scripts": {
    "app:webpack:start": "webpack-dev-server --hot --inline --progress --colors",
    "app:webpack:build": "webpack -p",
    "app:server:start": "babel-node scripts/server.js",
    "app:server:start:dev": "nodemon --exec babel-node -- scripts/server.js",
    "storage:db:migrate": "sequelize db:migrate --url $npm_package_config_storageDatabaseUri --models-path ./storage/models --migrations-path ./storage/migrations",
    "storage:db:rollback": "sequelize db:migrate:undo --url $npm_package_config_storageDatabaseUri --models-path ./storage/models --migrations-path ./storage/migrations",
    "storage:db:seed": "babel-node ./storage/seed.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel": "^5.8.23",
    "babel-runtime": "^5.8.25",
    "es6-promise": "^3.0.2",
    "express": "^4.13.3",
    "history": "^1.12.5",
    "passport-local": "^1.0.0",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-helmet": "^2.1.0",
    "react-redux": "^4.0.0",
    "react-router": "^1.0.0-rc3",
    "redux": "^3.0.4",
    "redux-logger": "^2.0.4",
    "redux-thunk": "^1.0.0"
  },
  "devDependencies": {
    "babel-loader": "^5.3.2",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.14.1",
    "classnames": "^2.2.0",
    "compression": "^1.6.0",
    "connect-mongo": "^0.8.2",
    "cookie-parser": "^1.4.0",
    "css-loader": "^0.19.0",
    "express-session": "^1.12.1",
    "extract-text-webpack-plugin": "^0.8.2",
    "install": "^0.2.3",
    "is-ok": "^1.0.1",
    "jquery": "^2.1.4",
    "jsonwebtoken": "^5.4.1",
    "lodash": "^3.10.1",
    "method-override": "^2.3.5",
    "mongoose": "^4.2.4",
    "morgan": "^1.6.1",
    "nib": "^1.1.0",
    "node-sass": "^3.4.1",
    "nodemon": "^1.7.1",
    "npm": "^3.3.10",
    "passport": "^0.3.0",
    "passport-http": "^0.3.0",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^3.1.1",
    "style-loader": "^0.12.4",
    "stylus-loader": "^1.4.0",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.0"
  }
}
